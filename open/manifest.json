{
	"manifest_version": 2,
	"name": "笑脸进件插件",
	"version": "1.0",
	"description": "笑脸测试环境进件专用插件",
	"author": "jianhao",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["https://baidu.com/*"],
			"js": ["jquery-1.8.3.js","content-script.js"]
		}
	],
	"web_accessible_resources": 
	[
		"inject.js"
	]
}